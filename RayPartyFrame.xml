			<Anchor point="BOTTOM" relativeTo="UIParent" relativePoint="BOTTOM" x="0" y="200"/>
		</Anchors>
		<KeyValues>
			<KeyValue key="spacing" value="10" type="number"/>
		</KeyValues>
		<Scripts>
			<OnLoad method="OnLoad" inherit="append"/>
			<OnShow method="OnShow"/>
			<OnEvent method="OnEvent"/>
		</Scripts>
	</Frame>
</Ui>
